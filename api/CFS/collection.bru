headers {
  client-id: {{client-id}}
}

auth {
  mode: bearer
}

auth:bearer {
  token: {{accessToken}}
}

script:post-response {
  // 로그인 성공 후
  if(res.status === 200){
    bru.setEnvVar('accessToken', res.body.item.token.accessToken);
    bru.setEnvVar('refreshToken', res.body.item.token.refreshToken);
  }
}
